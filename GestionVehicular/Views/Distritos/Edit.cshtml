@model GestionVehicular.Models.Distrito

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Distrito</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="DistritoId" />
            <div class="form-group">
                <label asp-for="Nombre" class="control-label"></label>
                <input asp-for="Nombre" class="form-control" />
                <span asp-validation-for="Nombre" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Provincia" class="control-label"></label>
                <input asp-for="Provincia" class="form-control" />
                <span asp-validation-for="Provincia" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NoDistrito" class="control-label"></label>
                <input asp-for="NoDistrito" class="form-control" />
                <span asp-validation-for="NoDistrito" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Parroquia" class="control-label"></label>
                <input asp-for="Parroquia" class="form-control" />
                <span asp-validation-for="Parroquia" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CodDistrito" class="control-label"></label>
                <input asp-for="CodDistrito" class="form-control" />
                <span asp-validation-for="CodDistrito" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="EsActivo" /> @Html.DisplayNameFor(model => model.EsActivo)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="FechaCreacion" class="control-label"></label>
                <input asp-for="FechaCreacion" class="form-control" />
                <span asp-validation-for="FechaCreacion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
