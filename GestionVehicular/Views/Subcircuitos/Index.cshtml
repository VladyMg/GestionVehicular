@model IEnumerable<GestionVehicular.Models.Subcircuito>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoSubcircuito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodSubcircuito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Circuito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EsActivo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCreacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoSubcircuito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodSubcircuito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Circuito.CodCircuito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EsActivo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCreacion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SubcircuitoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SubcircuitoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SubcircuitoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
