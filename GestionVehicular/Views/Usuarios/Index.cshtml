@page
@model GestionVehicular.Controllers.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].Cedula)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].Contrasenia)
