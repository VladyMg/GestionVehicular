@model IEnumerable<GestionVehicular.Models.Circuito>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoCircuito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodCircuito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DistritoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EsActivo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCreacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoCircuito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodCircuito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DistritoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EsActivo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCreacion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CircuitoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CircuitoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CircuitoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
